<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" stylesheets="@../css/yourPlans.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dailylist.dailylist.controller.YourPlansController">
   <children>
      <TableView id="plansTableView" fx:id="plansTableView" layoutX="147.0" layoutY="112.0" prefHeight="318.0" prefWidth="407.0">
        <columns>
          <TableColumn fx:id="nameColumn" editable="false" prefWidth="85.0" text="name" />
          <TableColumn fx:id="dateColumn" editable="false" prefWidth="99.0" text="date" />
            <TableColumn fx:id="dayColumn" editable="false" prefWidth="75.0" sortable="false" text="day" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button id="prevButton" fx:id="prevButton" alignment="CENTER" layoutX="2.0" layoutY="2.0" mnemonicParsing="false" onAction="#prevPage" prefHeight="17.0" prefWidth="11.0" text="&lt;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TextField fx:id="searchTextField" layoutX="196.0" layoutY="50.0" prefHeight="32.0" prefWidth="309.0" />
      <Button id="searchButton" fx:id="searchButton" layoutX="199.0" layoutY="53.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="24.0" textAlignment="CENTER" />
      <Button id="clearButton" fx:id="clearButton" layoutX="477.0" layoutY="53.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="24.0" />
   </children>
</AnchorPane>
