<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane stylesheets="@../css/justPlan.css" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dailylist.dailylist.controller.justPlanController">
   <children>
      <TableView id="planTable" fx:id="planTable" editable="true" layoutX="107.0" layoutY="50.0" nodeOrientation="LEFT_TO_RIGHT">
         <columns>
            <TableColumn id="timeColumn" fx:id="timeColumn" editable="false" maxWidth="66.0" minWidth="66.0" prefWidth="66.0" resizable="false" sortable="false" text="Time" />
            <TableColumn id="todoColumn" fx:id="todoColumn" maxWidth="418.0" minWidth="418.0" prefWidth="0.0" resizable="false" sortable="false" text="Todo" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button id="prevButton" fx:id="prevButton" alignment="CENTER" layoutX="8.0" layoutY="8.0" mnemonicParsing="false" onAction="#prevPage" prefHeight="17.0" prefWidth="11.0" text="&lt;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
